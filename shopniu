			var num = document.getElementById("second").innerHTML;
			var int = setInterval("count()", 1000);

			function count() {
				num--;
				document.getElementById("second").innerHTML = num;
				if (num == 0) {
					clearInterval(int);
					window.open('http://www.zptong.com', '_target');
				}
			}
		</script>
